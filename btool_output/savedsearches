/opt/splunk/etc/apps/SplunkVersionControl/default/savedsearches.conf      search = `comment("Query the audit logs to determine if the command | postversioncontrolrestore was used to send a request through")` 
index=_audit action=search info=granted `comment("For prod: savedsearch_name=\"search*\"")` user="$username$" `searchheadhosts`
| rex ", search='(?P<search>[\S+\s+]+?)', "
| regex search="^\| postversioncontrolrestore "
| eval time=strftime(_time,"%s")
| table time, user
